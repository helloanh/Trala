{{#each songData}}
  <div class="each-song column fourth">
  	<div class="song-header" >
      <p> {{title}} </p>
       {{#if artwork_url }}
      <img src="{{artwork_url}}"  id="{{id}}" class="clickable-song">
      {{else}}
      {{!-- display pic of empty track --}}
      <img src="http://static.midomi.com/images/no_album_art.jpg">
    {{/if}}
    </div>
  </div>
 {{/each}}
